{
  "background": {
    "scripts": ["./background.js"]
  },
  "browser_action": {

  },
  "name": "Bookmark Robot",
  "description": "Classify ur bookmark onCreated new bookmark, or creating classified folders by domain in one folder on exist bookmarks.",
  "icons": {
    "16": "assets/icons/icon.png",
    "32": "assets/icons/icon.png",
    "64": "assets/icons/icon.png",
    "128": "assets/icons/icon.png"
  },
  "manifest_version": 2,
  "options_ui": {
    "chrome_style": false,
    "page": "./options.html"
  },
  "permissions": [
    "background",
    "storage",
    "tabs",
    "bookmarks"
  ],
  "version": "1.0.0",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
